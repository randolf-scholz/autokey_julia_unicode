\DeclareOption{nostix}{\unicode@stixfalse}
\DeclareOption{fdsymbol}{\unicode@fdsymboltrue}
\DeclareOption{nofdsymbol}{\unicode@fdsymbolfalse}
\DeclareOption{mnsymbol}{\unicode@mnsymboltrue}
