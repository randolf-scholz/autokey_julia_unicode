\NewDocumentCommand{\unicode@makealias}{m O{} O{} O{} m}{%
% Create alias for existing command. Up to 3 options can be provided.
% \unicode@makealias{\alias}[\option1][\option2][\option3]{\fallback}
\ifdefined{#2}\let#1=#2%
\else\ifdefined{#3}\let#1=#3%
\else\ifdefined{#4}\let#1=#4%
\else\providecommand{#1}{#5}%
\fi\fi\fi%
}

% region font families -----------------------------------------------------------------
\newcommand{\unicode@normal}[1]{\ifmmode\mathnormal{#1}\else\textnormal{#1}\fi}
% Serif modifiers
\newcommand{\unicode@rm}[1]{\ifmmode\mathrm{#1}\else\textrm{#1}\fi}
\newcommand{\unicode@it}[1]{\ifmmode\mathit{#1}\else\textit{#1}\fi}
\newcommand{\unicode@bf}[1]{\ifmmode\mathbf{#1}\else\textbf{#1}\fi}
\newcommand{\unicode@bi}[1]{\ifmmode\boldsymbol{\mathit{#1}}\else\textbf{\textit{#1}}\fi}
% Sans-variants
\newcommand{\unicode@sf}[1]{\ifmmode\mathsf{#1}\else\textsf{#1}\fi}
\newcommand{\unicode@itsf}[1]{\ifmmode\mathsfit{#1}\else\textsf{\textit{#1}}\fi}
\newcommand{\unicode@bfsf}[1]{\ifmmode\boldsymbol{\mathsf{#1}}\else\textbf{\textsf{#1}}\fi}
\newcommand{\unicode@bisf}[1]{\ifmmode\boldsymbol{\mathsfit{#1}}\else\textbf{\textsf{\textit{#1}}}\fi}
% Fraktur
\newcommand{\unicode@frak}[1]{\ensuremath{\mathfrak{#1}}}
\newcommand{\unicode@bfrak}[1]{\ensuremath{\boldsymbol{\mathfrak{#1}}}}
% Script
\newcommand{\unicode@cal}[1]{\ensuremath{\mathcal{#1}}}
\newcommand{\unicode@scr}[1]{\ensuremath{\mathscr{#1}}}
% Misc
\newcommand{\unicode@tt}[1]{\ifmmode\mathtt{#1}\else\texttt{#1}\fi}
\newcommand{\unicode@bb}[1]{%
% We use mathds as primary, and fallback to mathbb.
\ifnum`#1=\uccode`#1\relax % Check if the character is uppercase
	\ensuremath{\mathds{#1}}% Use dsfont
\else\if#11% Check if the character is literal `1'
	\ensuremath{\mathds{#1}}%
\else% Use fallback
    \ensuremath{\mathbb{#1}}%
\fi\fi%
}
\newcommand{\unicode@bbi}[1]{\mathbb{\mathit{#1}}}
\newcommand{\unicode@circled}[1]{\textcircled{\footnotesize #1}}
