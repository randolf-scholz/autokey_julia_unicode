\newcommand*{\RomanNumber}[1]{%
  \relax\ifmmode\mathrm{\expandafter\@slowromancap\romannumeral #1@}% chktex 21
  \else{\rmfamily\expandafter\@slowromancap\romannumeral #1@}\fi% chktex 21
}
